{
  "name": "syncify",
  "version": "0.0.1",
  "main": "./main.js",
  "chromium-args": "--mixed-context",
  "node-remote": "http://localhost:5000",
  "homepage": ".",
  "window": {
    "title": "Syncify",
    "toolbar": true
  },  "scripts": {
    "dev": "concurrently \"npm run server\" \"cd client && npm run start\"",
    "start": "cd client && npm run build && cd .. && npm run server",
    "server": "nodemon server.js",
    "nw": "concurrently \"npm run start\" \"wait-on http://localhost:5000 && nw .\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "node-fetch": "^2.6.1",
    "node-hue-api": "^5.0.0-beta.2",
    "nodemon": "^2.0.7",
    "nw": "^0.52.0",
    "threads": "^1.6.3",
    "tiny-worker": "^2.3.0",
    "wait-on": "^5.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
